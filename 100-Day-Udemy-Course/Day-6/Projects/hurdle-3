#Hurdle 3
# Using our knoweldege on while + def

def turn_right():
    turn_left()
    turn_left()
    turn_left()

#the jump function changed a lil bit here
def jump():
    turn_left()    
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
    
#if wall is infront
while not at_goal():
    if wall_in_front():
        jump()
    else:
        move()
